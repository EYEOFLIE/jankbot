{
  "name": "jankbot",
  "version": "3.0.1",
  "description": "A Dota-centric steambot by JankDota",
  "main": "jankbot.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --recursive test",
    "ci": "npm run lint && npm run test",
    "coverage": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "lint": "eslint .",
    "release": "grunt release",
    "start": "node jankbot.js",
    "config": "node ./scripts/config.js"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4.2.0"
  },
  "dependencies": {
    "dota2": "3.0.0",
    "lodash": "4.6.1",
    "minimap": "0.1.1",
    "semver": "5.1.0",
    "steam": "1.4.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "^2.5.3",
    "grunt": "0.4.5",
    "grunt-cli": "1.1.0",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-compress": "1.1.1",
    "istanbul": "0.4.2",
    "load-grunt-tasks": "3.4.1",
    "mocha": "2.4.5",
    "mockery": "1.4.1",
    "sinon": "1.17.3"
  },
  "repository": "https://github.com/twisterghost/jankbot"
}
